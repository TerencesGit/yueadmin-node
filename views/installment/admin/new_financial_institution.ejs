<!DOCTYPE html>
<html>
  <head>
    <%- include('../../link') %>
  </head>
  <body>
    <%- include('./aside') %>
    <%- include('../../header') %>
    <div id="main" class="main" data-page="financial_manage">
      <div class="wrapper animated fadeInRight">
        <div class="container-fluid">
          <div class="panel panel-info">
            <div class="panel-heading">
              <h3 class="panel-title">
                <%=title%>
                <a href="javascript:history.go(-1)" class="pull-right">返回</a>
              </h3>
            </div> 
            <div class="modal-body">
              <form action="#" method="post" class="form-horizontal m-t">
                <div class="form-group row">
                  <label class="col-md-4 control-label">机构名称</label>
                  <div class="col-md-6">
                    <input type="text" name="" class="form-control" placeholder="机构名称">
                  </div>
                </div>
                <div class="form-group row">
                  <label class="col-md-4 control-label">机构别名</label>
                  <div class="col-md-6">
                    <input type="text" name="" class="form-control" placeholder="机构别名">
                  </div>
                </div>
                <div class="form-group row">
                  <label class="col-md-4 control-label">生效时间</label>
                  <div class="col-md-6">
                    <input type="text" id="start" class="form-control laydate-icon" placeholder="生效时间">
                  </div>
                </div>
                <div class="form-group row">
                  <label class="col-md-4 control-label">到期时间</label>
                  <div class="col-md-6">
                    <input type="text" id="end" class="form-control laydate-icon" placeholder="到期时间">
                  </div>
                </div>
                <div class="form-group row">
                  <label class="col-md-4 control-label">金服URL地址</label>
                  <div class="col-md-6">
                    <input type="text" name="" class="form-control" placeholder="金服URL地址">
                  </div>
                </div>
                <div class="form-group row">
                  <label class="col-md-4 control-label">回调URL地址</label>
                  <div class="col-md-6">
                    <input type="text" name="" class="form-control" placeholder="回调URL地址">
                  </div>
                </div>
                <div class="form-group row">
                  <label class="col-md-4 control-label">是否启用</label>
                  <div class="col-md-6">
                    <select name="" id="" class="form-control">
                      <option value="1">启用</option>
                      <option value="0">禁用</option>
                    </select>
                  </div>
                </div>
                <div class="form-group row">
                  <label class="col-md-4 control-label">展示排序</label>
                  <div class="col-md-6">
                    <input type="text" name="" class="form-control" placeholder="展示排序">
                  </div>
                </div>
                <div class="form-group row">
                  <label class="col-md-4 control-label">机构图标</label>
                  <div class="col-md-6">
                    <div class="preview-area w-120">
                      <input type="file" class="file-control">
                      <img src="/img/upload.png" class="img-preview" width="100" height="100" alt="图标上传" title="图标上传">
                      <a href="javascript:;" class="img-remove"></a>
                    </div>
                  </div>
                </div>
                <div class="form-group row">
                  <label class="col-md-4 control-label">备注</label>
                  <div class="col-md-6">
                    <textarea name="" class="form-control" rows="3" placeholder="备注"></textarea>
                  </div>
                </div>
                <div class="form-group row">
                  <div class="col-md-offset-6">
                    <button class="btn btn-success">提交</button>
                    <button class="btn btn-default m-l">取消</button>
                  </div>
                </div>
              </form>
            </div>
        </div>
        <div class="warp-footer"></div>
      </div>
    	<%- include('../../footer') %>
    </div>
    <%- include('../script')%>
  </body>
</html>
